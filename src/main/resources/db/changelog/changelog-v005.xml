<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.2.xsd">

    <changeSet id="5" author="Yakushev">

        <createTable tableName="partners">
            <column name="id" type="int" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"></constraints>
            </column>
            <column name="partner_name" type="varchar(200)">
                <constraints nullable="true" unique="false"/>
            </column>
            <column name="partner_inn" type="varchar(20)">
                <constraints nullable="true" unique="false"/>
            </column>
            <column name="partner_kpp" type="varchar(20)">
                <constraints nullable="true" unique="false"/>
            </column>
            <column name="partner_adress" type="varchar(200)">
                <constraints nullable="true" unique="false"/>
            </column>
            <column name="personal_manager" type="varchar(200)">
                <constraints nullable="true" unique="false"/>
            </column>
            <column name="manager_mobile_phone" type="varchar(20)">
                <constraints nullable="true" unique="false"/>
            </column>
            <column name="manager_city_phone" type="varchar(20)">
                <constraints nullable="true" unique="false"/>
            </column>
            <column name="manager_email" type="varchar(50)">
                <constraints nullable="true" unique="false"/>
            </column>
        </createTable>



    </changeSet>

</databaseChangeLog>