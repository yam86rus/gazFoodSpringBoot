<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.2.xsd">

    <changeSet id="8" author="Yakushev">

        <createTable tableName="cafeterias">
            <column name="id" type="int" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"></constraints>
            </column>
            <column name="name" type="varchar(100)">
                <constraints nullable="false" unique="false"/>
            </column>
            <column name="time_of_delivery" type="int">
                <constraints nullable="true" unique="false"/>
            </column>
            <column name="stars" type="DOUBLE">
                <constraints nullable="true" unique="false"/>
            </column>
            <column name="price" type="int">
                <constraints nullable="true" unique="false"/>
            </column>
            <column name="kitchen" type="varchar(200)">
                <constraints nullable="true" unique="false"/>
            </column>
            <column name="image" type="varchar(200)">
                <constraints nullable="true" unique="false"/>
            </column>
            <column name="products" type="varchar(200)">
                <constraints nullable="true" unique="false"/>
            </column>
            <column name="address" type="varchar(200)">
                <constraints nullable="false" unique="false"/>
            </column>
            <column name="phone" type="varchar(50)">
                <constraints nullable="true" unique="false"/>
            </column>
            <column name="active" type="BIT(1)">
<!--                <constraints nullable="true" unique="false"/>-->
            </column>
        </createTable>



    </changeSet>

</databaseChangeLog>